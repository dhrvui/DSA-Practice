def bubble_sort(arr):
    n = 0
    for _ in arr:
        n = n + 1

    i = 0
    while i < n:
        j = 0
        while j < n - i - 1:
            if arr[j] > arr[j + 1]:
                temp = arr[j]
                arr[j] = arr[j + 1]
                arr[j + 1] = temp
            j = j + 1
        i = i + 1

    return arr

arr = [ 64, 52, 33, 21, 8, 43]
print(bubble_sort(arr))
