class Stack:
    def __init__(self, size):
        self.size = size
        self.items = [0] * size
        self.top = -1

    def push(self, value):
        if self.top == self.size - 1:
            return "Stack Overflow"
        self.top += 1
        self.items[self.top] = value
    
    def pop(self):
        if self.top == -1:
            return "Stack Underflow"
        value = self.items[self.top]
        self.top -= 1
        return value
    
    def peek(self):
        if self.top == -1:
            return "Stack Underflow"
        return self.items[self.top]
    
    def is_empty(self):
        return self.top == -1
    

stack = Stack(5)
stack.push(10)
stack.push(20)
stack.push(30)
print(stack.pop())
print(stack.peek())
