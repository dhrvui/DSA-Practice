arr = [1,2,3,4]

def max_element(arr):
    max = arr[0]
    for i in range(len(arr)):
        if(max < arr[i]):
            max = arr[i]
    print(max)

def min_element(arr):
    min = arr[0]
    for i in range(len(arr)):
        if(arr[i] < min):
            min = arr[i]
    print(min)

max_element(arr)
min_element(arr)
